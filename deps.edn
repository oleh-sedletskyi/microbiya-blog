{:paths ["src" "resources"]
 :deps {;; org.clojure/clojure {:mvn/version "1.12.1"}
        ;; md->html
        markdown-clj/markdown-clj {:mvn/version "1.12.2"}
        hiccup/hiccup {:mvn/version "2.0.0-RC5"}
        org.mariadb.jdbc/mariadb-java-client {:mvn/version "3.5.5"}
        com.github.seancorfield/next.jdbc {:mvn/version "1.3.1048"}
        ;; cleanup html
        org.jsoup/jsoup  {:mvn/version "1.21.2"}
        com.clojure-goes-fast/clj-memory-meter {:mvn/version "0.3.0"}
